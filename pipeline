pipeline{

	agent any
	environment{
		RELEASE='20.4'
		
	}
	
	
	stages{
	
	
			stage('Build'){
				agent any
				environment {
					echo "Building release with log level.."
				}
				
				steps{
					echo "Building release ${RELEASE} with log level"
				}
			}
		
			stage('Test'){
				steps{
					echo "Testing. I can see release ${RELEASE}, but not log level..."
				}
			}
	
	
		}

}